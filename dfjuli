	$('#game').fadeOut();
					  }, 4000)	
			}
for (var i = 0, len = die.length; i < len; i++) {
  var rond = getRandom(max, min);
  console.log(rond); 
  setDie(die[i], rond);
}
function getRandom(max, min) { 
  return (Math.floor(Math.random() * (max - min)) + min);
}

